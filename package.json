{
  "name": "costal-hazard-intelligence",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:client": "next dev",
    "dev:server": "nodemon --exec \"ts-node --transpile-only -P tsconfig.server.json\" server/index.ts",
    "build": "npm run build:server && npm run build:client",
    "build:client": "next build",
    "build:server": "tsc -p tsconfig.server.json",
    "start": "node dist/server/index.js",
    "start:client": "next start",
    "lint": "eslint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "seed": "ts-node --transpile-only scripts/seed-demo-data.ts"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@types/leaflet": "^1.9.21",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "leaflet": "^1.9.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "next": "16.1.1",
    "next-auth": "^4.24.13",
    "openai": "^6.16.0",
    "pg": "^8.11.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-leaflet": "^5.0.0",
    "recharts": "^3.6.0",
    "redis": "^4.6.10",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.7.4",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20",
    "@types/pg": "^8.10.9",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^9.0.7",
    "concurrently": "^8.2.2",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "fast-check": "^3.15.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "tailwindcss": "^4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5"
  }
}
